{
  "name": "mock-shop",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "db": "babel-node ./src/data-access/config/index.js",
    "db_pro": "node ./dist/data-access/config/index.js",
    "test": "nyc mocha --require @babel/register ./test --exit",
    "start-up": "yarn init && yarn run start-up1 && yarn start-up2",
    "start-up1": "yarn add cors dotenv express morgan jsonwebtoken bcryptjs",
    "start-up2": "yarn add --dev mocha chai",
    "server": "node ./src/index.js",
    "build": "yarn run clean && babel src -d dist",
    "now-build": "yarn run build",
    "clean": "rimraf dist",
    "dev": "yarn run db && nodemon --exec babel-node ./src/index.js",
    "start": "yarn run clean && yarn run build && yarn run db_pro && node ./dist/index.js"
  },
  "author": "Akinlua Bolamigbe Jomiloju <bolamigbeakinlua@gmail.com>",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.8.6",
    "axios": "^0.21.1",
    "babel-plugin-inline-dotenv": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.2",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "datauri": "^3.0.0",
    "dataurl": "^0.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.5",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "faker": "^4.1.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1"
  },
  "license": "MIT",
  "description": "Mock Shop is a simple shopping server",
  "repository": "https://github.com/alob-mtc/mock-shop-server.git"
}
